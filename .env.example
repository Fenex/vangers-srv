# Example environment variables for Vangers Server
# Copy to ".env" or export these in your environment.

# Server port (default: 2197)
VANGERS_PORT=2197

# Set to "true" to suppress SERVER_TIME_* log events (default: false)
VANGERS_SUPRESS_LOG_SERVER_TIME=false

# Set to "true" to suppress GAMES_LIST_QUERY log events (default: false)
VANGERS_SUPRESS_LOG_GAMES_LIST_QUERY=false

# Optional: control tracing_subscriber log level (trace|debug|info|warn|error)
RUST_LOG=info
